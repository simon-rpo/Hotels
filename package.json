    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.10.0"
  },
  "keywords": [
    "node",
    "express",
    "create-react-app"
  ]
}
